    "type": "symfony-bundle",
    "version": "1.1.4",
    "license": "MIT",
    "authors": [
        {
            "name": "Sylvestre",
            "email": "sylvestre.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/config": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/security-bundle": "^6.0|^7.0",
        "doctrine/orm": "^3.0",
        "doctrine/doctrine-bundle": "^2.10",
        "symfony/uid": "^6.0|^7.0"
